doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		meta(name="description", content="Easy way to load a bunch of js libraries into the console for testing and play.")

		link(rel="icon", type="image/ico", href="./favicon.ico")
		link(href="https://fonts.googleapis.com/icon?family=Material+Icons", rel="stylesheet")

		title js lib playground

		script(src="./main.bundle.js", type="text/javascript", async, defer)

		script(src="https://cdnjs.cloudflare.com/ajax/libs/babel-standalone/6.10.3/babel.min.js", type="text/javascript", async, defer)
	body(ng-cloak).absolute-fill
		#dom
		div(data-ng-view).fill
